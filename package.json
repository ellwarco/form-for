    "babel-plugin-transform-flow-comments": "^6.22.0",
    "flow-bin": "^0.57.3",
    "nwb": "0.19.x",
    "prettier": "^1.7.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "author": "Pedro Moreira",
  "homepage": "pedrosm.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pedsmoreira/form-for.git"
  },
  "keywords": [
    "react-component",
    "form",
    "simple-form",
    "react",
